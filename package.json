    "react-autocomplete": "^1.8.1",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chai": "^4.1.2",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "2.29.0",
    "husky": "^0.14.3",
    "jest": "20.0.4",
    "json-server": "^0.12.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.4.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-test-renderer": "^16.2.0",
    "selenium-standalone": "^6.13.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "wdio-junit-reporter": "^0.4.2",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.4",
    "webdriverio": "^4.12.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "selenium-setup": "selenium-standalone install",
    "selenium-start": "selenium-standalone start > /dev/null 2>&1 &",
    "selenium-stop": "pkill -f selenium-standalone",
    "integration-test": "wdio wdio.conf.js",
    "integration-test-watch": "wdio wdio.conf.js --watch",
    "dev-server":
      "json-server --routes ./routes.json --watch ./currency.db.json --port 3001 --read-only --quiet",
    "start:dev": "npm run dev-server & node scripts/start.js",
    "start:live":
      "export REACT_APP_BASE_URL='http://coincap.io' && node scripts/start.js",
    "start": "npm run start:live",
    "build": "node scripts/build.js",
    "predeploy":
      "export REACT_APP_BASE_URL='https://api.gregoftheweb.com' npm run build",
    "deploy": "gh-pages -d build",
    "test": "node scripts/test.js --env=jsdom",
    "spec": "npm run test -- --verbose --coverage",
    "precommit": "pretty-quick --staged"
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.{js,jsx,mjs}"],
    "coveragePathIgnorePatterns": [
      "src/index.js",
      "src/registerServiceWorker.js",
      "src/store"
    ],
    "setupFiles": ["<rootDir>/config/polyfills.js"],
    "testMatch": ["<rootDir>/specs/**/?(*.)(spec|test).{js,jsx,mjs}"],
    "setupTestFrameworkScriptFile": "<rootDir>/specs/setupTests.js",
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)":
        "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": ["react-app"]
  }
}
